{
    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (web API with watch)",
			"type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/SmartRouting/bin/Debug/net9.0/SmartRouting.dll",
            "args": ["watch"],
            "cwd": "${workspaceFolder}/SmartRouting",
            "stopAtEntry": false,
			"requireExactSource": false,
            "serverReadyAction": {
                "action": "openExternally",
                "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
            },
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "sourceFileMap": {
                "/Views": "${workspaceFolder}/Views"
            },
            "launchSettingsProfile": "http",
            "postDebugTask": "terminate-port-5123"


         
       
           
